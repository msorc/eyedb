JAVAC = CLASSPATH=$(EYEDBCLASSPATH) javac

all: classes/Store.class

classes/%.class: %.java
	$(JAVAC) $<

classes/Store.class: Store.java
	$(JAVAC) *.java

clean:
	rm -f classes/*.class

init:
	@if [ ! -d classes ]; then mkdir classes; fi
