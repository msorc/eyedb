<!ENTITY eyedbdbdelete "<command>eyedbdbdelete</command>">
]>

<refentry id="eyedbdbdelete">
  <refentryinfo>
    <date>March 2007</date>
  </refentryinfo>

  <refmeta>
    <refentrytitle>eyedbdbdelete</refentrytitle>
    <manvolnum>1</manvolnum>
    <refmiscinfo>eyedb database</refmiscinfo>
  </refmeta>

  <refnamediv>
    <refname>&eyedbdbdelete;</refname>
    <refpurpose>Deletes an EyeDB database.</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <cmdsynopsis>
      &eyedbdbdelete;
      <arg choice="opt"><replaceable>common-options</replaceable></arg> 
      <arg choice="plain"><replaceable>dbname</replaceable></arg>
    </cmdsynopsis>
  </refsynopsisdiv>

  <refsect1>
    <title>Description</title>

    <para>&eyedbdbdelete; is used to delete an EyeDB database.</para>
  </refsect1>

  <refsect1>
    <title>Options</title>

    <variablelist>

      <varlistentry>
	<term><arg choice="plain"><replaceable>dbname</replaceable></arg></term>
	<listitem>
	  <para>Specifies the name of the database to be deleted.</para>
	</listitem>
      </varlistentry>

    </variablelist>

  </refsect1>

  <refsect1>
    <title>See also</title>

    <para>
      eyedb-options(1), eyedbdbcreate(1), eyedbadmin(1)
    </para>
  </refsect1>

</refentry>
