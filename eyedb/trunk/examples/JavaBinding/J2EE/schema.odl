class Person {
  attribute string firstname;
  attribute string lastname;
  attribute int age;
  relationship set<Car *> cars inverse Car::owner;

  index on firstname;
  index on lastname;
  index on age;
};

class Car {
  attribute string model;
  attribute int num;
  relationship Person *owner inverse Person::cars;
};
