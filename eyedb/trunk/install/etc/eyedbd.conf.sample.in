# EyeDB server configuration file
# 
# This file is a template that you can use to override the default values.

# Bases directory
#datadir = @localstatedir@/eyedb/db;

# Temporary files directory
#tmpdir = @localstatedir@/eyedb/tmp;

# Path to .so files
#sopath = @libdir@/eyedb;

# Default EYEDBDBM database
#default_dbm = @localstatedir@/eyedb/db/dbmdb.dbs;

# Granted EYEDBDBM database
#granted_dbm = %default_dbm%;

# Default file mask
#default_file_mask = "0";

# Default file group
#default_file_group = "";

# Maximum server memory size in Mb
#maximum_memory_size = 0;

# Server access file
#access_file = @sysconfdir@/eyedb/Access;

# Smd connection port
#smdport = @localstatedir@/eyedb/pipes/eyedbsmd;

# Server listen ports and sockets list
#listen = localhost:6240,@localstatedir@/eyedb/pipes/eyedbd;

# OQL files path
#oqlpath = @libdir@/eyedb/oql
